{
  "name": "Marko",
  "short_name": "Marko",
  "description": "A sophisticated bookmarking application",
  "categories": ["business", "productivity", "utilities", "lifestyle"],
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "orientation": "portrait-primary",
  "share_target": {
    "action": "/marko/newfile",
    "method": "GET",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "icons": [
    {
    "src": "triskelion.svg",
    "sizes": "48x48 72x72 96x96 128x128 256x256 512x512",
    "type": "image/svg+xml",
    "purpose": "any"
    }
  ],
  "start_url": "/marko/newfile",
  "scope": "/marko",
  "display": "standalone",
  "background_color": "#ffa426",
  "theme_color": "#0d47a1"
}

 
