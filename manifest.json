{
  "name": "Marko",
  "short_name": "Marko",
  "description": "A sophisticated bookmarking application",
  "categories": ["business", "productivity", "utilities", "lifestyle"],
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "orientation": "portrait-primary",
  "share_target": {
    "action": "/marko/newfile",
    "method": "GET",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "icons": [
    {
      "src": "icon.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "icon.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "start_url": "/marko/newfile",
  "scope": "/marko",
  "display": "standalone",
  "background_color": "#ffa426",
  "theme_color": "#0d47a1"
}

 
